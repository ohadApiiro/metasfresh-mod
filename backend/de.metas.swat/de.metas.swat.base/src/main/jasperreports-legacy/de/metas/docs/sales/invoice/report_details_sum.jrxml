				</textElement>
				<textFieldExpression><![CDATA[$F{source}.equals("01_TL")?
    $R{netsum}:
    ($F{source}.equals("04_PS")?
        $R{compoints}:
    	($F{source}.equals("02_STX")?
            ($F{istaxincluded}.equals("Y")?
                $R{VATincluded}:
		$R{plus} + $R{VAT}
            ):
            ($F{source}.equals("02_TX")?
                ($F{istaxincluded}.equals("Y")?
                    $F{tax} + "% " + $R{VATincluded}:
                    $R{plus} + $F{tax} + "% " + $R{VAT}
                ):
		$R{totalsum}
            )
	)
     )]]></textFieldExpression>
			</textField>
