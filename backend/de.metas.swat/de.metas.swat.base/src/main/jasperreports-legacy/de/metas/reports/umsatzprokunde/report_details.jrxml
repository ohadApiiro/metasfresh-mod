	<parameter name="C_BPartner_ID" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="monate_drucken" isForPrompting="true" class="java.lang.String"/>
	<parameter name="prodkat_drucken" isForPrompting="true" class="java.lang.String"/>
	<parameter name="produkte_drucken" isForPrompting="true" class="java.lang.String"/>
	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<queryString><![CDATA[SELECT
	bp_name,		bp_value,	
	monat,			--jahr,
	pc_name,		p_value,
	p_name,			cursymbol,
	sum(umsatz0) as priceactual_now,
	sum(umsatz1) as priceactual_last,
	sum(umsatz2) as priceactual_before_last
FROM (
	SELECT
		produktumsatz.ad_org_id,		
		produktumsatz.bp_name,
		produktumsatz.bp_value,			
		produktumsatz.c_bpartner_id,
		(CASE WHEN $P{monate_drucken} LIKE 'Y' THEN produktumsatz.monat ELSE NULL END)as monat,			
		produktumsatz.jahr,
		(CASE WHEN $P{prodkat_drucken} LIKE 'Y' THEN produktumsatz.pc_name ELSE NULL END)as pc_name,
		produktumsatz.p_value,
		produktumsatz.p_name,			
		produktumsatz.cursymbol,
		CASE WHEN produktumsatz.jahrdiff = 0 THEN produktumsatz.umsatz ELSE 0 END AS umsatz0,
		CASE WHEN produktumsatz.jahrdiff = 1 THEN produktumsatz.umsatz ELSE 0 END AS umsatz1,
		CASE WHEN produktumsatz.jahrdiff = 2 THEN produktumsatz.umsatz ELSE 0 END AS umsatz2
	FROM
	(
		SELECT 
			i.ad_org_id,
			cur.cursymbol,	
			bp.name as bp_name,
			bp.value as bp_value,
			bp.c_bpartner_id,
			EXTRACT('month' FROM i.dateinvoiced) as monat,
			EXTRACT('year' FROM i.dateinvoiced) as jahr,
			EXTRACT('year' FROM now()) - EXTRACT('year' FROM i.dateinvoiced) as jahrdiff,
			pc.name as pc_name,		
			p.value as p_value,	
			p.name as p_name,
			invoiceline.umsatz					
		FROM
		(--selektion aller rechnungszeilen
			SELECT
				il.c_invoice_id,
				il.m_product_id,
				sum(il.linenetamt) as umsatz
			FROM 
				c_invoiceline il
			WHERE
				ad_org_id::text LIKE '%'||$P{AD_Org_ID}||'%'
			GROUP BY
				il.c_invoice_id,
				il.m_product_id
			ORDER BY
				il.m_product_id	
		) as invoiceline
		JOIN m_product p		ON invoiceline.m_product_id = p.m_product_id
		JOIN m_product_category pc 	ON p.m_product_category_id = pc.m_product_category_id
		JOIN c_invoice i		ON invoiceline.c_invoice_id = i.c_invoice_id
		JOIN c_bpartner bp		ON i.c_bpartner_id = bp.c_bpartner_id
		JOIN c_currency cur		ON i.c_currency_id = cur.c_currency_id
	)as produktumsatz
)as kundenumsatz
WHERE
	c_bpartner_id::text LIKE '%'||$P{C_BPartner_ID}||'%'
	AND ad_org_id::text LIKE '%'||$P{AD_Org_ID}||'%'
GROUP BY
	bp_name,
	bp_value,
	monat,
	--jahr,
	pc_name,
	p_value,
	p_name,
	cursymbol
ORDER BY
	bp_value,	
	monat,
	pc_name,
	p_name	
;]]></queryString>

