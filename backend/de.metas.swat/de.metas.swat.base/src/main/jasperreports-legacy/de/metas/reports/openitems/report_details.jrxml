	<parameter name="IsSOTrx" isForPrompting="true" class="java.lang.String"/>
	<parameter name="DaysDue" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal(500)]]></defaultValueExpression>
	</parameter>
	<parameter name="C_BPartner_ID" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="InvoiceCollectionType" isForPrompting="true" class="java.lang.String"/>
	<parameter name="AD_Org_ID" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal (1000002)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 
	oi.const, 	oi.documentno, 	oi.dateinvoiced,	oi.netdays, 		oi.discountdays, 
	oi.duedate, 	oi.daysdue, 	oi.discountdate,	oi.grandtotal,		oi.paidamt,
	oi.openamt,	bp.value,	bp.name,		o.name as orgname, 	l.address1, 
	l.city, 	l.postal,	cur.cursymbol

FROM
	(SELECT 
		'a' AS const, i.ad_org_id, i.ad_client_id, i.documentno, i.c_invoice_id, i.c_order_id, i.c_bpartner_id, 
		i.issotrx, i.dateinvoiced, i.dateacct, 
		p.netdays, p.discountdays,
		paymenttermduedate(i.c_paymentterm_id, i.dateinvoiced::timestamp with time zone) AS duedate, 
		paymenttermduedays(i.c_paymentterm_id, i.dateinvoiced::timestamp with time zone, getdate()) AS daysdue, 
		adddays(i.dateinvoiced::timestamp with time zone, p.discountdays) AS discountdate, 
		round(i.grandtotal * p.discount / 100::numeric, 2) AS discountamt, i.grandtotal, 
		invoicepaid(i.c_invoice_id, i.c_currency_id, 1::numeric) AS paidamt, 
		invoiceopen(i.c_invoice_id, 0::numeric) AS openamt, 
		i.c_currency_id, i.c_conversiontype_id, i.c_paymentterm_id, i.ispayschedulevalid, NULL::numeric AS c_invoicepayschedule_id, i.invoicecollectiontype, i.c_campaign_id, i.c_project_id, i.c_activity_id
	FROM 
		rv_c_invoice i
		JOIN c_paymentterm p ON i.c_paymentterm_id = p.c_paymentterm_id
	WHERE 
		invoiceopen(i.c_invoice_id, 0::numeric) <> 0::numeric 
		AND i.ispayschedulevalid <> 'Y'::bpchar 
		AND i.docstatus not  in ('DR','VO','RE')
	UNION 
	SELECT 
		'b' as const, i.ad_org_id, i.ad_client_id, i.documentno, i.c_invoice_id, i.c_order_id, i.c_bpartner_id, 
		i.issotrx, i.dateinvoiced, i.dateacct, 
		daysbetween(ips.duedate::timestamp with time zone, i.dateinvoiced::timestamp with time zone) AS netdays,  null,
		ips.duedate, 
		daysbetween(getdate(), ips.duedate::timestamp with time zone) AS daysdue, 
		ips.discountdate, 
		ips.discountamt, ips.dueamt AS grandtotal, 
		ips.dueamt -  invoiceopen(i.c_invoice_id, ips.c_invoicepayschedule_id) AS paidamt,  
		invoiceopen(i.c_invoice_id, ips.c_invoicepayschedule_id) AS openamt, i.c_currency_id, i.c_conversiontype_id, i.c_paymentterm_id, i.ispayschedulevalid, ips.c_invoicepayschedule_id, i.invoicecollectiontype, i.c_campaign_id, i.c_project_id, i.c_activity_id
	FROM 
		rv_c_invoice i
		JOIN c_invoicepayschedule ips ON i.c_invoice_id = ips.c_invoice_id
	WHERE 
		invoiceopen(i.c_invoice_id, ips.c_invoicepayschedule_id) > 0::numeric 
		AND i.ispayschedulevalid = 'Y'::bpchar 
		AND i.docstatus <> 'DR'::bpchar 
		AND ips.isvalid = 'Y'::bpchar
	)as oi
	JOIN c_bpartner bp 	ON oi.c_bpartner_id =bp.c_bpartner_id
	JOIN ad_org o 		ON o.ad_client_id = oi.ad_client_id
	JOIN ad_orginfo ori 	ON ori.ad_org_id = o.ad_org_id
	JOIN c_location l 	ON l.c_location_id = ori.c_location_id
	JOIN c_currency cur 	ON oi.c_currency_id = cur.c_currency_id
WHERE
	oi.ad_org_id::text LIKE '%'||$P{AD_Org_ID}||'%'
	AND oi.issotrx LIKE '%'||$P{IsSOTrx}||'%'
	AND oi.c_bpartner_id::text LIKE '%'||$P{C_BPartner_ID}||'%'
	AND oi.daysdue <= $P{DaysDue}
	AND
	CASE 
		WHEN oi.invoicecollectiontype is null 
		THEN '' 
		ELSE oi.invoicecollectiontype
	END LIKE '%'||$P{InvoiceCollectionType}||'%'

order by oi.daysdue desc, oi.dateinvoiced, oi.documentno
;]]></queryString>

