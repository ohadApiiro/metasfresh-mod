	<parameter name="von" isForPrompting="true" class="java.util.Date"/>
	<parameter name="bis" isForPrompting="true" class="java.util.Date"/>
	<parameter name="Tagessumme" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Monatssumme" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Wochensumme" isForPrompting="true" class="java.lang.String"/>
	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<queryString><![CDATA[SELECT 
	'' as const, 		o.name as orgname, 
	l.address1, 		l.city, 
	l.postal, 		cur.cursymbol, 
	i.created, 		i.c_invoice_id, 
	bp.value, 		bp.name, 
	i.documentno,		i.ad_client_id, 
	sum(il.linenetamt)	AS linenetamt, 
	sum(il.linelistamt)	AS linelistamt, 
	sum(il.linelimitamt)	AS linelimitamt, 
	sum(il.linediscountamt)	AS linediscountamt, 
	date_trunc('day',i.created) AS daycreated,
	EXTRACT('week' FROM i.created) AS weekcreated,
	date_trunc('month', i.created) AS monat,
        CASE
            WHEN sum(il.linelistamt) = 0::numeric THEN 0::numeric
            ELSE round((sum(il.linelistamt) - sum(il.linenetamt)) / sum(il.linelistamt) * 100::numeric, 2)
        END AS linediscount, 
        sum(il.lineoverlimitamt) AS lineoverlimitamt, 
        CASE
            WHEN sum(il.linenetamt) = 0::numeric THEN 0::numeric
            ELSE 100::numeric - round((sum(il.linenetamt) - sum(il.lineoverlimitamt)) / sum(il.linenetamt) * 100::numeric, 2)
        END AS lineoverlimit
FROM 
	c_invoice i
	JOIN c_doctype d 	   ON i.c_doctype_id = d.c_doctype_id
	JOIN c_bpartner bp 	   ON i.c_bpartner_id = bp.c_bpartner_id
	JOIN c_currency cur 	   ON i.c_currency_id = cur.c_currency_id
	JOIN ad_org o 		   ON o.ad_client_id = i.ad_client_id
	JOIN ad_orginfo oi 	   ON oi.ad_org_id = o.ad_org_id
	JOIN c_location l 	   ON l.c_location_id = oi.c_location_id
	JOIN rv_c_invoiceline il   ON il.c_invoice_id = i.c_invoice_id
WHERE
	i.ad_org_id::text LIKE '%'||$P{AD_Org_ID}||'%'
	AND Date_trunc('day',i.created) >= $P{von}
	AND Date_trunc('day',i.created) <= $P{bis}
group by o.name, l.address1, l.city, l.postal, i.c_invoice_id, cur.cursymbol, i.created, bp.value, bp.name, i.documentno, i.ad_client_id
order by i.documentno, i.created
;]]></queryString>

