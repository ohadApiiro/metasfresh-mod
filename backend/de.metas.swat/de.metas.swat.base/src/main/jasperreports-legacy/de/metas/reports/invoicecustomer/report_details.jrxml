	<parameter name="von" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT 
	*
FROM	
	(SELECT
		--rechnung
		i.documentno,
		i.created,
		cur.cursymbol,
		--bp
		bp.value 		as customerno,
		bp.name 		as customername,
		ev.value 		as account,
		--product
		p.name			AS prodname,
	 	p.value			AS prodno, 
		ev.value 		as konto,
		--rechnungszeile
		il.qtyinvoiced, 	
	 	il.linenetamt		AS brutto,
	 	il.taxamt		AS steuer,
	 	il.linenetamt + il.taxamt AS netto,
		t.rate 			as tax
	FROM
		c_invoice i
		JOIN c_invoiceline il 		ON i.c_invoice_id 	= il.c_invoice_id
		JOIN c_currency cur 		ON i.c_currency_id 	= cur.c_currency_id
		JOIN c_invoicetax it		ON i.c_invoice_id	= it.c_invoice_id
		JOIN c_bpartner	bp		ON i.c_bpartner_id	= bp.c_bpartner_id
		JOIN c_bp_customer_acct ca 	ON i.c_bpartner_id 	= ca.c_bpartner_id
		JOIN c_tax t 			ON il.c_tax_id 		= t.c_tax_id
		JOIN m_product p		ON il.m_product_id 	= p.m_product_id
		JOIN m_product_acct pa 		ON p.m_product_id 	= pa.m_product_id
		JOIN c_validcombination vc 	ON pa.p_revenue_acct 	= vc.c_validcombination_id
		JOIN c_validcombination bpvc 	ON ca.c_receivable_acct = bpvc.c_validcombination_id
		JOIN c_elementvalue bpev 	ON bpvc.account_id 	= bpev.c_elementvalue_id
		JOIN c_elementvalue ev 		ON vc.account_id 	= ev.c_elementvalue_id
	WHERE
		i.ad_client_id = 1000000
	)as foo
WHERE
	foo.created >= $P{von}
	and foo.created<=$P{bis}
GROUP BY 
		documentno,
		created,
		cursymbol,
		customerno,
		customername,
		account,
		--product
		prodname,
	 	prodno, 
		konto,
		--rechnungszeile
		qtyinvoiced, 	
	 	brutto,
	 	steuer,
	 	netto,
		tax

ORDER BY
	documentno,
	customerno,
	tax,
	prodname
;]]></queryString>

