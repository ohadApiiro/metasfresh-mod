	<queryString>
		<![CDATA[SELECT
	o.DocumentNo,
	o.DateOrdered,
	COALESCE( dtt.PrintName, dt.PrintName ) as dt_documenttype
FROM
	C_Order o
	LEFT OUTER JOIN C_DocType dt ON o.C_DocType_ID = dt.C_DocType_ID AND dt.isActive = 'Y'
	LEFT OUTER JOIN C_DocType_Trl dtt ON dt.C_DocType_ID = dtt.C_DocType_ID AND dtt.AD_Language = $P{ad_language} AND dtt.isActive = 'Y'
WHERE
	o.C_Order_ID = $P{RECORD_ID} AND o.isActive = 'Y'
;]]>
