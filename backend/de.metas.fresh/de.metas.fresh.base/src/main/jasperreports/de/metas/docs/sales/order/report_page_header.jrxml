	<queryString>
		<![CDATA[SELECT
	COALESCE( dtt.PrintName, dt.PrintName ) AS PrintName,
	o.DateOrdered,
	o.Documentno
FROM
	C_Order o
	LEFT OUTER JOIN C_DocType dt ON o.C_DocType_ID = dt.C_DocType_ID AND dt.isActive = 'Y'
	LEFT OUTER JOIN C_DocType_Trl dtt ON dt.C_DocType_ID = dtt.C_DocType_ID AND dtt.isActive = 'Y'
		AND dtt.AD_Language = $P{ad_language}
WHERE
	o.C_Order_ID = $P{RECORD_ID} AND o.isActive = 'Y'
;]]>
	</queryString>
