	<variable name="sum_ein" class="java.lang.Float" calculation="Sum">
		<variableExpression><![CDATA[$F{einnahme}]]></variableExpression>
	</variable>
	<variable name="sum_aus" class="java.lang.Float" calculation="Sum">
